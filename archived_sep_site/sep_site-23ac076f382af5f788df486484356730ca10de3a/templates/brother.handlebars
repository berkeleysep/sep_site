{{#each brothers}}
  <div class='col-lg-3 col-md-3 col-sm-3 col-xs-6'>
    <a href={{linkedin}}>

      <div class='brothers-pic montserrat-light'>
        <img src={{img_src}} class='img-responsive'>
        <div class='brothers-caption'>
          <h5>{{name}}</h5>
          <hr class='brothers-caption-divider'>
          <h6>{{major}}</h6>
          <h6>{{year}}</h6>
        </div>
      </div>

    </a>
  </div>
{{/each}}
